        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "ionic:build:fast4": "node --max_old_space_size=8048 npm install -g @ionic/cli && node --max_old_space_size=8048 node_modules/@ionic/cli/bin/ionic capacitor add android && node --max_old_space_size=8048 node_modules/@ionic/cli/bin/ionic capacitor copy android && cd android && ./gradlew assembleDebug",
        "ionic:build:fast3": "ionic capacitor run android --target adb-2e590ca9-8hGWKy._adb-tls-connect._tcp",
        "ionic:build:fast": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build app --configuration=fastbuild",
        "ionic:build:fast2": "ionic capacitor sync android",
        "ionic:cap:add:android":"ionic capacitor add android",
        "ionic:cap:add:ios":"capacitor add ios",
        "ionic:cap:build:android":"ionic capacitor build android --no-open --no-build --prod",
        "ionic:cap:build:ios":"ionic capacitor build ios --no-open --no-build",
        "ionic:cap:apk:android":"cd android && ./gradlew assembleDebug",
        "ionic:cap:ipa:ios":"cd android && ./gradlew assembleDebug",
        "ionic:cap:assets:android":"capacitor-assets generate --assetPath ../../resources --android",
        "ionic:cap:assets:ios":"capacitor-assets generate --assetPath ../../resources --ios",
        "ionic:build:watch": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build app --configuration=fastbuild --watch=true",
        "ionic:build:prod": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build app --configuration=production",
        "ionic:serve": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng serve app --hmr=true"
    },
